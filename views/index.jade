extends layout

block content
  h1= title
  p This is Sequelize greeting you :)

  h2 Creating a new user
  form(action="/users/create", method="post")
    input(type="number", name="type")
    input(type="text", name="name")
    input(type="text", name="email")
    input(type="text", name="postal")
    input(type="submit")

  h2 Users

  ul
  each user in users
    li
      = user.email

